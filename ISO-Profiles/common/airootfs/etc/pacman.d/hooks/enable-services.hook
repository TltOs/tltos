# remove from airootfs!
[Trigger]
Operation = Install
Type = Package
Target = irqbalance

[Action]
Description = Enabling Performance Tweaking Services ...
When = PostTransaction
Depends = sh
Depends = ananicy-cpp
Depends = preload
Depends = irqbalance
Depends = haveged
Depends = uresourced
Exec = /bin/sh -c "systemctl enable irqbalance ananicy-cpp preload haveged avahi-daemon uresourced ufw touchegg"
